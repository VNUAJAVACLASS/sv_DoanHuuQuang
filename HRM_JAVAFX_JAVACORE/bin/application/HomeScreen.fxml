							<VBox.margin>
								<Insets bottom="5.0" />
							</VBox.margin>
						</Label>
						<DatePicker fx:id="birthdayPicker" prefHeight="26.0"
							prefWidth="271.0" />
					</children>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</VBox>
				<VBox layoutX="20.0" layoutY="212.0" prefWidth="258.0">
					<children>
						<Label text="Khóa học đăng ký" textFill="#0000009b">
							<VBox.margin>
								<Insets bottom="5.0" />
							</VBox.margin>
						</Label>
						<ChoiceBox fx:id="courseChoiceBox" prefWidth="270.0" />
					</children>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</VBox>
				<VBox layoutX="20.0" layoutY="255.0" prefWidth="258.0">
					<children>
						<Label text="Mật khẩu đăng nhập" textFill="#0000009b">
							<VBox.margin>
								<Insets bottom="5.0" />
							</VBox.margin>
						</Label>
						<TextField fx:id="passwordTextField" />
					</children>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</VBox>
				<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
					spacing="20.0">
					<children>
						<Button fx:id="addBtn" mnemonicParsing="false"
							onMouseClicked="#onClickAddButton" prefHeight="26.0"
							prefWidth="74.0" text="Thêm" />
						<Button fx:id="updateBtn" layoutX="10.0" layoutY="10.0"
							mnemonicParsing="false" onMouseClicked="#onClickUpdateButton"
							prefHeight="26.0" prefWidth="82.0" text="Cập nhật" />
