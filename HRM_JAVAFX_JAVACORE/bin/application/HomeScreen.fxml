							<VBox.margin>
								<Insets bottom="5.0" />
							</VBox.margin>
						</Label>
						<HBox alignment="CENTER_LEFT" prefHeight="100.0"
							prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1"
							GridPane.rowIndex="3">
							<children>
								<RadioButton fx:id="maleRadioButton" text="Nam">
									<toggleGroup>
										<ToggleGroup fx:id="genderGroup" />
									</toggleGroup>
								</RadioButton>
								<RadioButton fx:id="femaleRadioButton" text="Nữ"
									toggleGroup="$genderGroup" />
							</children>
						</HBox>
					</children>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</VBox>
				<VBox layoutX="20.0" layoutY="158.0" prefWidth="258.0">
