			maxWidth="393.199951171875" minWidth="10.0"
			prefWidth="271.9999877929687" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="161.8000244140625"
			minHeight="0.0" prefHeight="54.399993896484375" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="415.20000610351565"
			minHeight="10.0" prefHeight="415.20000610351565" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="330.40003051757816"
			minHeight="10.0" prefHeight="30.4000244140625" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<HBox alignment="CENTER" prefHeight="54.0" prefWidth="693.0"
			style="-fx-background-color: yellow;" GridPane.halignment="CENTER"
			GridPane.valignment="CENTER">
			<children>
				<Label alignment="CENTER" contentDisplay="RIGHT"
					text="ỨNG DỤNG QUẢN LÝ THÀNH VIÊN CLB TIN HỌC" textFill="#003cf2">
					<font>
						<Font name="System Bold" size="18.0" />
					</font>
				</Label>
			</children>
		</HBox>
		<HBox alignment="CENTER" layoutX="10.0" layoutY="10.0"
			prefHeight="54.0" prefWidth="269.0"
			style="-fx-background-color: yellow;" GridPane.columnIndex="1">
			<children>
				<Label fx:id="welcomeLabel" contentDisplay="CENTER"
					text="Xin chào" />
				<Label contentDisplay="RIGHT" onMouseClicked="#onClickLogout"
					text="Đăng xuất" textFill="#ff00008b">
					<HBox.margin>
						<Insets left="10.0" />
					</HBox.margin>
					<font>
						<Font name="System Bold" size="12.0" />
					</font>
				</Label>
			</children>
		</HBox>
		<VBox prefHeight="200.0" prefWidth="100.0"
			GridPane.columnIndex="1" GridPane.rowIndex="1">
			<children>
				<Label text="Thông tin chi tiết">
					<font>
						<Font name="System Bold" size="18.0" />
					</font>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</Label>
				<VBox prefWidth="258.0">
					<children>
						<Label text="Email" textFill="#0000009b">
							<VBox.margin>
								<Insets bottom="5.0" />
							</VBox.margin>
						</Label>
						<TextField fx:id="emailTextField" />
					</children>
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</VBox>
				<VBox layoutX="20.0" layoutY="51.0" prefWidth="258.0">
