							mnemonicParsing="false" onMouseClicked="#onClickDeleteButton"
							prefHeight="26.0" prefWidth="77.0" text="Xóa" />
					</children>
					<opaqueInsets>
						<Insets />
					</opaqueInsets>
				</HBox>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</VBox>
		<TableView fx:id="userListTableView"
			onMouseClicked="#onRowClick" prefHeight="200.0" prefWidth="200.0"
			GridPane.rowIndex="1">
			<columns>
				<TableColumn fx:id="idCol"
					prefWidth="103.99996793270111" text="Mã sinh viên" />
				<TableColumn fx:id="emailCol"
					prefWidth="83.20001983642578" text="Email" />
				<TableColumn fx:id="fullnameCol"
					prefWidth="174.39996337890625" text="Họ tên" />
				<TableColumn fx:id="courseCol"
					prefWidth="159.20001831054685" text="Khóa học đăng ký" />
			</columns>
		</TableView>
	</children>
